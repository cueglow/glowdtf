<?xml version="1.0"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:t="http://schemas.gdtf-share.com/device" targetNamespace="http://schemas.gdtf-share.com/device">
  <!-- updated to 0.88 + robe attributes-->
  <xs:element name="GDTF">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="t:FixtureType"/>
      </xs:sequence>
      <xs:attribute name="DataVersion" type="t:dataversion" use="required"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="FixtureType">
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="t:AttributeDefinitions"/>
        <xs:element ref="t:Wheels"/>
        <xs:element ref="t:PhysicalDescriptions"/>
        <xs:element ref="t:Models"/>
        <xs:element ref="t:Geometries"/>
        <xs:element ref="t:DMXModes"/>
        <xs:element ref="t:Revisions"/>
        <xs:element ref="t:FTPresets"/>
        <xs:element ref="t:Protocols"/>
      </xs:choice>
      <xs:attribute name="Name" type="t:nametype" use="required"/>
      <xs:attribute name="ShortName" type="xs:string" use="optional"/>
      <xs:attribute name="LongName" type="xs:string" use="optional"/>
      <xs:attribute name="Manufacturer" type="xs:string" use="required"/>
      <xs:attribute name="Description" type="xs:string" use="required"/>
      <xs:attribute name="FixtureTypeID" type="t:guidtype" use="required"/>
      <xs:attribute name="Thumbnail" type="xs:string" use="optional"/>
      <xs:attribute name="RefFT" type="t:guidtype" use="optional"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="Wheels">
    <xs:complexType>
      <xs:sequence minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="t:Wheel"/>
      </xs:sequence>
    </xs:complexType>
    <xs:unique name="UniqueWheelName">
      <xs:selector xpath="t:Wheel"/>
      <xs:field xpath="@Name"/>
    </xs:unique>
  </xs:element>
  <xs:element name="Wheel">
    <xs:complexType>
      <xs:sequence minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="t:Slot"/>
      </xs:sequence>
      <xs:attribute name="Name" type="t:nametype" use="optional"/>
    </xs:complexType>
    <xs:unique name="UniqueSlotName">
      <xs:selector xpath="t:Slot"/>
      <xs:field xpath="@Name"/>
    </xs:unique>
  </xs:element>
  <xs:element name="Slot">
    <xs:complexType>
      <xs:sequence minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="t:Facet"/>
      </xs:sequence>
      <xs:attribute name="Name" type="t:nametype" use="required"/>
      <xs:attribute name="Color" type="t:colorcietype" use="optional"/>
      <xs:attribute name="Filter" type="xs:string" use="optional"/>
      <xs:attribute name="MediaFileName" type="xs:string" use="optional"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="Facet">
    <xs:complexType>
      <xs:attribute name="Color" type="t:colorcietype" use="optional"/>
      <xs:attribute name="Rotation" type="xs:string" use="optional"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="Models">
    <xs:complexType>
      <xs:sequence minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="t:Model"/>
      </xs:sequence>
    </xs:complexType>
    <xs:unique name="UniqueModel">
      <xs:selector xpath="t:Model"/>
      <xs:field xpath="@Name"/>
    </xs:unique>
  </xs:element>
  <xs:element name="Model">
    <xs:complexType>
      <xs:attribute name="Name" type="t:nametype" use="required"/>
      <xs:attribute name="Length" type="xs:float" use="optional"/>
      <xs:attribute name="Width" type="xs:float" use="optional"/>
      <xs:attribute name="Height" type="xs:float" use="optional"/>
      <xs:attribute name="PrimitiveType" type="t:PrimitiveTypeEnum" use="optional"/>
      <xs:attribute name="File" type="xs:string" use="optional"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="Geometries">
    <xs:complexType>
      <xs:sequence minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="t:Geometry" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="t:Axis" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="t:FilterBeam" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="t:FilterColor" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="t:FilterGobo" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="t:FilterShaper" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="t:Beam" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="t:GeometryReference" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
    </xs:complexType>
    <xs:unique name="UniqueGeometry">
      <xs:selector xpath="t:Geometry"/>
      <xs:field xpath="@Name"/>
    </xs:unique>
    <xs:unique name="UniqueAxis">
      <xs:selector xpath="t:Axis"/>
      <xs:field xpath="@Name"/>
    </xs:unique>
    <xs:unique name="UniquFilterBeam">
      <xs:selector xpath="t:FilterBeam"/>
      <xs:field xpath="@Name"/>
    </xs:unique>
    <xs:unique name="UniqueFilterColor">
      <xs:selector xpath="t:FilterColor"/>
      <xs:field xpath="@Name"/>
    </xs:unique>
    <xs:unique name="UniqueFilterGobo">
      <xs:selector xpath="t:FilterGobo"/>
      <xs:field xpath="@Name"/>
    </xs:unique>
    <xs:unique name="UniqueFilterShaper">
      <xs:selector xpath="t:FilterShaper"/>
      <xs:field xpath="@Name"/>
    </xs:unique>
    <xs:unique name="UniqueBeam">
      <xs:selector xpath="t:FilterBeam"/>
      <xs:field xpath="@Name"/>
    </xs:unique>
    <xs:unique name="UniqueGeometryReference">
      <xs:selector xpath="t:GeometryReference"/>
      <xs:field xpath="@Name"/>
    </xs:unique>
  </xs:element>
  <xs:element name="Geometry">
    <xs:complexType>
      <xs:sequence minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="t:Geometry" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="t:Axis" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="t:FilterBeam" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="t:FilterColor" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="t:FilterGobo" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="t:FilterShaper" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="t:Beam" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="t:GeometryReference" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attribute name="Name" type="t:nametype" use="optional"/>
      <xs:attribute name="Model" type="t:nametype" use="optional"/>
      <xs:attribute name="Position" type="t:matrixtype" use="optional"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="Axis">
    <xs:complexType>
      <xs:sequence minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="t:Geometry" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="t:Axis" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="t:FilterBeam" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="t:FilterColor" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="t:FilterGobo" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="t:FilterShaper" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="t:Beam" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="t:GeometryReference" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attribute name="Name" type="t:nametype" use="optional"/>
      <xs:attribute name="Model" type="t:nametype" use="optional"/>
      <xs:attribute name="Position" type="t:matrixtype" use="optional"/>
      <!--removed in 1.0      <xs:attribute name="From" type="xs:string" use="optional"/>
      <xs:attribute name="To" type="xs:string" use="optional"/>
      <xs:attribute name="Speed" type="xs:string" use="optional"/>-->
    </xs:complexType>
  </xs:element>
  <xs:element name="FilterBeam">
    <xs:complexType>
      <xs:sequence minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="t:Geometry" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="t:Axis" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="t:FilterBeam" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="t:FilterColor" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="t:FilterGobo" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="t:FilterShaper" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="t:Beam" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="t:GeometryReference" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attribute name="Name" type="t:nametype" use="optional"/>
      <xs:attribute name="Model" type="t:nametype" use="optional"/>
      <xs:attribute name="Position" type="t:matrixtype" use="optional"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="FilterColor">
    <xs:complexType>
      <xs:sequence minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="t:Geometry" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="t:Axis" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="t:FilterBeam" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="t:FilterColor" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="t:FilterGobo" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="t:FilterShaper" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="t:Beam" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="t:GeometryReference" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attribute name="Name" type="t:nametype" use="optional"/>
      <xs:attribute name="Model" type="t:nametype" use="optional"/>
      <xs:attribute name="Position" type="t:matrixtype" use="optional"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="FilterGobo">
    <xs:complexType>
      <xs:sequence minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="t:Geometry" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="t:Axis" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="t:FilterBeam" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="t:FilterColor" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="t:FilterGobo" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="t:FilterShaper" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="t:Beam" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="t:GeometryReference" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attribute name="Name" type="t:nametype" use="optional"/>
      <xs:attribute name="Model" type="t:nametype" use="optional"/>
      <xs:attribute name="Position" type="t:matrixtype" use="optional"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="FilterShaper">
    <xs:complexType>
      <xs:sequence minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="t:Geometry" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="t:Axis" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="t:FilterBeam" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="t:FilterColor" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="t:FilterGobo" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="t:FilterShaper" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="t:Beam" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="t:GeometryReference" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attribute name="Name" type="t:nametype" use="optional"/>
      <xs:attribute name="Model" type="t:nametype" use="optional"/>
      <xs:attribute name="Position" type="t:matrixtype" use="optional"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="Beam">
    <xs:complexType>
      <xs:sequence minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="t:Geometry" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="t:Axis" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="t:FilterBeam" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="t:FilterColor" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="t:FilterGobo" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="t:FilterShaper" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="t:Beam" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="t:GeometryReference" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attribute name="Name" type="t:nametype" use="required"/>
      <xs:attribute name="Model" type="t:nametype" use="required"/>
      <xs:attribute name="Position" type="t:matrixtype" use="required"/>
      <xs:attribute name="LampType" type="t:LampTypeEnum" use="optional"/>
      <xs:attribute name="PowerConsumption" type="xs:float" use="optional"/>
      <xs:attribute name="LuminousFlux" type="xs:float" use="optional"/>
      <xs:attribute name="ColorTemperature" type="xs:float" use="optional"/>
      <xs:attribute name="BeamAngle" type="xs:float" use="optional"/>
      <xs:attribute name="FieldAngle" type="xs:float" use="optional"/>
      <xs:attribute name="BeamRadius" type="xs:float" use="optional"/>
      <xs:attribute name="BeamType" type="t:BeamTypeEnum" use="optional"/>
      <xs:attribute name="ColorRenderingIndex" type="xs:integer" use="optional"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="GeometryReference">
    <xs:complexType>
      <xs:sequence minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="t:Break"/>
      </xs:sequence>
      <xs:attribute name="Name" type="t:nametype" use="optional"/>
      <xs:attribute name="Position" type="t:matrixtype" use="optional"/>
      <xs:attribute name="Geometry" type="t:nametype" use="optional"/>
      <xs:attribute name="Model" type="t:nametype" use="optional"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="Break">
    <xs:complexType>
      <xs:attribute name="DMXOffset" type="xs:integer" use="optional"/>
      <xs:attribute name="DMXBreak" type="xs:integer" use="optional"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="DMXModes">
    <xs:complexType>
      <xs:sequence minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="t:DMXMode"/>
      </xs:sequence>
    </xs:complexType>
    <xs:unique name="UniqueDMXModeName">
      <xs:selector xpath="t:DMXMode"/>
      <xs:field xpath="@Name"/>
    </xs:unique>
  </xs:element>
  <xs:element name="DMXMode">
    <xs:complexType>
      <xs:sequence minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="t:DMXChannels"/>
        <xs:element ref="t:Relations" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="t:FTMacros" minOccurs="0" maxOccurs="1"/>
      </xs:sequence>
      <xs:attribute name="Name" type="t:nametype" use="required"/>
      <xs:attribute name="Geometry" type="t:nametype" use="optional"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="DMXChannels">
    <xs:complexType>
      <xs:sequence minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="t:DMXChannel"/>
      </xs:sequence>
    </xs:complexType>
    <xs:unique name="UniqueAttributeinGeometry">
      <xs:selector xpath="t:DMXChannel"/>
      <xs:field xpath="@Geometry"/>
      <xs:field xpath="t:LogicalChannel/@Attribute"/>
    </xs:unique>
  </xs:element>
  <xs:element name="DMXChannel">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="t:LogicalChannel"/>
      </xs:sequence>
      <xs:attribute name="Name" type="t:nametype" use="optional"/>
      <xs:attribute name="DMXBreak" type="xs:integer" use="optional"/>
      <xs:attribute name="Offset" type="t:offsettype" use="optional"/>
      <!--reworked in 1.0      <xs:attribute name="Coarse" type="t:dmxvalue" use="optional"/>
      <xs:attribute name="Fine" type="t:dmxvalue" use="optional"/>

      <xs:attribute name="Ultra" type="t:dmxvalue" use="optional"/>
      <xs:attribute name="Uber" type="t:dmxvalue" use="optional"/>-->
      <!-- removed in 0.90      <xs:attribute name="Frequency" type="t:FrequencyEnum" use="optional"/> -->
      <xs:attribute name="Default" type="t:dmxtype" use="optional"/>
      <xs:attribute name="Highlight" type="t:dmxtype" use="optional"/>
      <xs:attribute name="Geometry" type="xs:string" use="optional"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="LogicalChannel">
    <xs:complexType>
      <xs:sequence minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="t:ChannelFunction"/>
      </xs:sequence>
      <xs:attribute name="Name" type="t:nametype" use="optional"/>
      <xs:attribute name="Attribute" type="t:AttributeEnum" use="required"/>
      <xs:attribute name="Snap" type="t:SnapEnum" use="optional"/>
      <xs:attribute name="Master" type="t:MasterEnum"/>
      <xs:attribute name="MibFade" type="xs:float" use="optional"/>
      <xs:attribute name="DMXChangeTimeLimit" type="xs:float" use="optional"/>
    </xs:complexType>
    <xs:unique name="UniqueChannelFunctionAttributeAndDMXFrom">
      <xs:selector xpath="t:ChannelFunction"/>
      <xs:field xpath="@Attribute"/>
      <xs:field xpath="@DMXFrom"/>
      <xs:field xpath="@ModeFrom"/>
    </xs:unique>
    <xs:unique name="UniqueChannelFunction">
      <xs:selector xpath="t:ChannelFunction"/>
      <xs:field xpath="@Name"/>
    </xs:unique>
  </xs:element>
  <xs:element name="ChannelFunction">
    <xs:complexType>
      <xs:sequence minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="t:ChannelSet"/>
      </xs:sequence>
      <xs:attribute name="Name" type="xs:string" use="optional"/>
      <xs:attribute name="Attribute" type="t:AttributeEnum" use="required"/>
      <xs:attribute name="OriginalAttribute" type="xs:string" use="optional"/>
      <xs:attribute name="DMXFrom" type="t:dmxtype" use="optional"/>
      <xs:attribute name="PhysicalFrom" type="xs:float" use="optional"/>
      <xs:attribute name="PhysicalTo" type="xs:float" use="optional"/>
      <xs:attribute name="RealFade" type="xs:float" use="optional"/>
      <xs:attribute name="Wheel" type="xs:string" use="optional"/>
      <xs:attribute name="Emitter" type="xs:string" use="optional"/>
      <xs:attribute name="Filter" type="xs:string" use="optional"/>
      <!-- removed 1.0      <xs:attribute name="DMXInvert" type="t:YesNoEnum" use="optional"/> -->
      <!--      <xs:attribute name="EncoderInvert" type="t:YesNoEnum" use="optional"/>-->
      <xs:attribute name="ModeMaster" type="xs:string" use="optional"/>
      <xs:attribute name="ModeFrom" type="t:dmxtype" use="optional" default="0/1"/>
      <xs:attribute name="ModeTo" type="t:dmxtype" use="optional"/>
    </xs:complexType>
    <xs:unique name="UniqueChannelSetNameAndDMXFrom">
      <xs:selector xpath="t:ChannelSet"/>
      <xs:field xpath="@Name"/>
      <xs:field xpath="@DMXFrom"/>
    </xs:unique>
  </xs:element>
  <xs:element name="ChannelSet">
    <xs:complexType>
      <xs:attribute name="Name" type="xs:string" use="optional"/>
      <xs:attribute name="DMXFrom" type="t:dmxtype" use="optional"/>
      <xs:attribute name="DMXTo" type="t:dmxtype" use="optional"/>
      <xs:attribute name="PhysicalFrom" type="xs:float" use="optional"/>
      <xs:attribute name="PhysicalTo" type="xs:float" use="optional"/>
      <!--      <xs:attribute name="RealFade" type="xs:float" use="optional"/> -->
      <xs:attribute name="WheelSlotIndex" type="xs:integer" use="optional"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="Relations">
    <xs:complexType>
      <xs:sequence minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="t:Relation"/>
      </xs:sequence>
    </xs:complexType>
    <xs:unique name="UniqueRelationName">
      <xs:selector xpath="t:Relation"/>
      <xs:field xpath="@Name"/>
    </xs:unique>
  </xs:element>
  <xs:element name="Relation">
    <xs:complexType>
      <xs:attribute name="Name" type="xs:string" use="required"/>
      <xs:attribute name="Master" type="xs:string" use="required"/>
      <xs:attribute name="Follower" type="xs:string" use="required"/>
      <xs:attribute name="Type" type="t:RelationTypesEnum" use="required"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="Revisions">
    <xs:complexType>
      <xs:sequence minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="t:Revision"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="Revision">
    <xs:complexType>
      <xs:attribute name="Text" type="xs:string" use="optional"/>
      <xs:attribute name="Date" type="xs:dateTime" use="optional"/>
      <xs:attribute name="UserID" type="xs:integer" use="optional"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="FTPresets">
    <xs:complexType>
      <xs:sequence minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="t:FTPreset"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="FTPreset">
    <xs:complexType/>
  </xs:element>
  <xs:element name="FTMacros">
    <xs:complexType>
      <xs:sequence minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="t:FTMacro" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="FTMacro">
    <xs:complexType>
      <xs:sequence minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="t:MacroDMX" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="t:MacroVisual" minOccurs="0" maxOccurs="1"/>
      </xs:sequence>
      <xs:attribute name="Name" type="xs:string" use="optional"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="MacroDMX">
    <xs:complexType>
      <xs:sequence minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="t:DMXStep" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="DMXStep">
    <xs:complexType>
      <xs:sequence minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="t:DMXValue" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attribute name="Duration" type="xs:integer" use="optional"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="DMXValue">
    <xs:complexType>
      <xs:attribute name="Value" type="t:dmxtype" use="optional"/>
      <xs:attribute name="DMXChannel" type="xs:string" use="optional"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="MacroVisual">
    <xs:complexType>
      <xs:sequence minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="t:VisualStep" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="VisualStep">
    <xs:complexType>
      <xs:sequence minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="t:VisualValue" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
      <xs:attribute name="Duration" type="xs:integer" use="optional"/>
      <xs:attribute name="Fade" type="xs:string" use="optional"/>
