{
  "name": "cueglow-webui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@blueprintjs/core": "^3.44.2",
    "@blueprintjs/popover2": "^0.11.1",
    "@blueprintjs/select": "^3.16.2",
    "@hookform/resolvers": "^2.6.1",
    "@reach/router": "^1.3.4",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^13.1.8",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.170",
    "@types/node": "^15.0.2",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.3",
    "@types/styled-components": "^5.1.11",
    "@types/tabulator-tables": "^4.9.1",
    "@types/uuid": "^8.3.0",
    "http-proxy-middleware": "^2.0.0",
    "lodash": "^4.17.21",
    "node-sass": "^5.0.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-hook-form": "^7.11.0",
    "react-scripts": "^4.0.3",
    "react-tabulator": "^0.15.0",
    "styled-components": "^5.3.0",
    "tabulator-tables": "^4.9.3",
    "typescript": "^4.2.4",
    "uuid": "^8.3.2",
    "zod": "^3.5.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "cy": "cypress open",
    "e2e": "cypress open --config baseUrl=http://localhost:7000"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.ts?(x)"
        ],
        "rules": {
          "@typescript-eslint/no-explicit-any": "warn"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/reach__router": "^1.3.7",
    "cypress": "^8.3.1",
    "cypress-file-upload": "^5.0.7",
    "cypress-plugin-tab": "^1.0.5",
    "mock-socket": "^9.0.3"
  },
  "jest": {
    "moduleNameMapper": {
      "sass-extract-loader?.*": "<rootDir>/__mocks__/sass-extract-loader.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!@blueprintjs)/"
    ]
  }
}
